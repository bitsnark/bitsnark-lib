{
  "name": "bitsnark",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "keywords": [
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "npm run emulate-setup && jest",
    "py-install": "./python/run-in-venv.sh echo done",
    "generate-tx": "ts-node ./src/agent/setup/init-templates.ts",
    "generate-tx-clear": "ts-node ./src/agent/setup/init-templates.ts --clear",
    "generate-scripts": "ts-node ./src/agent/setup/generate-scripts.ts",
    "add-amounts": "ts-node ./src/agent/setup/amounts.ts",
    "sign-tx": "./python/run-in-venv.sh ts-node ./src/agent/setup/sign-transactions.ts",
    "verify-setup": "ts-node ./src/agent/setup/verify-setup.ts",
    "compile": "tsc",
    "lint": "eslint --ignore-pattern node_modules/ --ignore-pattern dist/ --ignore-pattern python/ .",
    "lint-fix": "npm run lint -- --fix",
    "prettier-check": "./scripts/prettier.sh",
    "prettier-fix": "./scripts/prettier.sh --fix",
    "tx-chart": "npm run generate-tx && ./analysis/generate-dot.sh",
    "start-db": "./scripts/start-db.sh",
    "start-prover": "./python/run-in-venv.sh ts-node ./src/agent/setup/agent.ts bitsnark_prover_1",
    "start-verifier": "./python/run-in-venv.sh ts-node ./src/agent/setup/agent.ts bitsnark_verifier_1",
    "start-regtest": "./scripts/start-regtest.sh",
    "stop-node": "docker stop bitcoin-node",
    "emulate-setup": "./python/run-in-venv.sh ts-node ./src/agent/setup/emulate-setup.ts",
    "emulate-setup-final": "./python/run-in-venv.sh ts-node ./src/agent/setup/emulate-setup.ts --final",
    "python-test": "cd python && ./run-in-venv.sh pytest"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@bitauth/libauth": "3.0.0",
    "axios": "1.7.4",
    "bigint-isqrt": "0.3.2",
    "bitcoin-core": "4.2.0",
    "bitcoinjs-lib": "6.1.6",
    "bscript-parser": "0.1.0",
    "crypto-js": "4.2.0",
    "dotenv": "^16.4.5",
    "ecpair": "2.1.0",
    "hash-wasm": "4.11.0",
    "pg-format": "1.0.4",
    "sitka": "1.1.1",
    "snarkjs": "0.7.4",
    "telegraf": "4.16.3",
    "tiny-secp256k1": "2.2.3",
    "ts-graphviz": "2.1.4",
    "ts-postgres": "2.0.4",
    "websnark": "git+https://github.com/tornadocash/websnark.git#4c0af6a8b65aabea3c09f377f63c44e7a58afa6d"
  },
  "devDependencies": {
    "@eslint/js": "9.9.1",
    "@types/crypto-js": "4.2.2",
    "@types/eslint__js": "8.42.3",
    "@types/jest": "29.5.12",
    "@types/node": "20.16.10",
    "@types/pg-format": "1.0.5",
    "@types/sinon": "17.0.3",
    "@types/snarkjs": "0.7.8",
    "conventional-changelog-cli": "5.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.9.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "29.7.0",
    "merkletreejs": "0.4.0",
    "nodemon": "3.1.4",
    "nyc": "17.0.0",
    "prettier": "^3.3.3",
    "sinon": "18.0.0",
    "source-map-support": "0.5.21",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "typescript-eslint": "8.3.0"
  }
}
